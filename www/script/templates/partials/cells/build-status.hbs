<div>
    <span class="hide">{{text.[0]}}</span>
    <a href="{{url}}">#{{number}}</a>
</div>

{{#if build_tags.length}}
    <div>
        {{#each build_tags}}
            <span class="label label-warning" title="{{this.description}}">{{this.title}}</span>
        {{/each}}
    </div>
{{/if}}

<span class="status-text">
    {{#text}}
        {{.}}
    {{/text}}
</span>
